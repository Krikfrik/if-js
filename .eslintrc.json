{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "func-names": 0,
        "max-classes-per-file": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": 0,
        "consistent-return": 0,
        "no-plusplus": 0
    }
}
